<fest:template xmlns:fest="http://fest.mail.ru" context_name="json">
    <div id="header" class="grid__str_1 main__head" style = "background-color:#e26d5c;">
        <img class = "grid__col-offset_2" src = "/images/logo.png" style="height:100%"></img>
        <button class="js-over" type="submit">Конец хода</button>

        <button class="js-exit" type="submit">Выход</button>

        <button class="js-pass" type="submit">Пасс</button>

        <button class="js-debugConclude" type="submit">debug_conclude</button>
    </div>

    <img id="loader" src="/images/ajax-loader.gif" class = "grid__str-offset-4 grid__col-offset_5" style="position:absolute"></img>
    <div  class = "grid__container grid__str_9">
        <div id = "myModal" class="modal fade js-modal" tabindex="-1" role="dialog">
            <div class="modal-dialog">
                <div class="modal-content">
                    <div class="modal-header">
                        <h4 class="modal-title"></h4>
                    </div>
                    <div class="modal-body">
                        <p class="js-alert"></p>
                        <p class="js-gamer1"></p>
                        <p class="js-gamer2"></p>
                        <p class="js-gamer3"></p>
                        <p class="js-gamer4"></p>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-default" data-dismiss="modal">Вернуться</button>
                        <button type="button" class="btn btn-primary js-exit">Выход</button>
                    </div>
                </div><!-- /.modal-content -->
            </div><!-- /.modal-dialog -->
        </div><!-- /.modal -->
        <div class = "grid__col_9 grid__str_10">
            <canvas id="canvas" class="grid__str_10 grid__col_12" style="position:absolute;overflow: hidden;z-index: 1000;"></canvas>
            <div class = "grid__str_8"></div>
        </div>
    </div>

</fest:template>
